      ]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "https://api.performance-analyzer.com/analyze",
        "qs": {
          "integration": "integration_d",
          "timeframe": "1h",
          "metrics": "response_time,throughput,error_rate,resource_usage"
        },
        "options": {
          "response": {
            "response": {
              "responseFormat": "json"
            }
