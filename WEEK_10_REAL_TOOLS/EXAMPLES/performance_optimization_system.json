        "options": {
          "response": {
            "response": {
              "responseFormat": "json"
            }
          }
        }
      },
      "name": "Analyze Integration B Performance",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
