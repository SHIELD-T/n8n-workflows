            {
              "name": "company_size",
              "value": "={{ $json.company_size }}"
            },
            {
              "name": "industry",
              "value": "={{ $json.industry }}"
            },
            {
              "name": "pain_points",
              "value": "={{ $json.pain_points }}"
            }
          ]
        }
      },
      "name": "Process Lead Data",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [
        460,
        300
      ]
    },
    {
      "parameters": {
        "resource": "chat",
        "operation": "create",
        "model": "gpt-4",
        "messages": {
          "values": [
            {
              "role": "system",
              "content": "You are a client acquisition specialist that analyzes leads and generates personalized outreach strategies."
            },
            {
              "role": "user",
              "content": "={{ 'Analyze this lead for client acquisition:\\nCompany: ' + $json.company_name + '\\nContact: ' + $json.contact_name + '\\nIndustry: ' + $json.industry + '\\nCompany Size: ' + $json.company_size + '\\nPain Points: ' + $json.pain_points + '\\n\\nGenerate: lead qualification score, recommended service package, personalized outreach message, and next steps.' }}"
            }
          ]
        }
      },
      "name": "AI Lead Analysis",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [
        680,
        300
      ]
    },
    {
      "parameters": {
        "operation": "create",
        "base": "your_base_id",
        "table": "Client Leads",
        "fields": {
          "Lead ID": "={{ $json.lead_id }}",
          "Lead Source": "={{ $json.lead_source }}",
          "Company Name": "={{ $json.company_name }}",
          "Contact Name": "={{ $json.contact_name }}",
          "Email": "={{ $json.email }}",
          "Phone": "={{ $json.phone }}",
          "Company Size": "={{ $json.company_size }}",
          "Industry": "={{ $json.industry }}",
          "Pain Points": "={{ $json.pain_points }}",
          "AI Analysis": "={{ $json.choices[0].message.content }}",
          "Status": "New",
          "Created At": "={{ $now }}"
        }
      },
      "name": "Store Lead Data",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        900,
        300
      ]
    },
    {
      "parameters": {
        "operation": "send",
        "toEmail": "={{ $json.email }}",
        "subject": "={{ 'Automation Opportunity for ' + $json.company_name }}",
        "message": "={{ 'Hi ' + $json.contact_name + ',\\n\\nI noticed ' + $json.company_name + ' is in the ' + $json.industry + ' industry, and I believe there\\'s a significant opportunity to automate your processes and save your team valuable time.\\n\\nBased on your company size and industry, I\\'ve helped similar businesses save 10+ hours per week through automation.\\n\\nWould you be interested in a brief 15-minute call to discuss how automation could benefit ' + $json.company_name + '?\\n\\nBest regards,\\n[Your Name]' }}"
      },
      "name": "Send Initial Outreach",
      "type": "n8n-nodes-base.gmail",
      "typeVersion": 1,
      "position": [
        1120,
        300
      ]
    },
    {
      "parameters": {
        "operation": "postMessage",
        "channel": "#sales-leads",
        "text": "={{ 'ðŸŽ¯ New Lead Generated\\nCompany: ' + $json.company_name + '\\nContact: ' + $json.contact_name + '\\nIndustry: ' + $json.industry + '\\nCompany Size: ' + $json.company_size + '\\nPain Points: ' + $json.pain_points + '\\nLead ID: ' + $json.lead_id }}"
      },
      "name": "Notify Sales Team",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        1340,
        300
      ]
    },
    {
      "parameters": {
        "operation": "create",
        "databaseId": "your_database_id",
        "fields": {
          "Title": "={{ 'Follow up with ' + $json.contact_name + ' (' + $json.company_name + ')' }}",
          "Lead ID": "={{ $json.lead_id }}",
          "Company": "={{ $json.company_name }}",
          "Contact": "={{ $json.contact_name }}",
          "Industry": "={{ $json.industry }}",
          "Priority": "High",
          "Due Date": "={{ $now.plus({ hours: 24 }) }}",
          "Status": "To Do",
          "Assigned To": "Sales Team"
        }
      },
      "name": "Create Follow-up Task",
      "type": "n8n-nodes-base.notion",
      "typeVersion": 1,
      "position": [
        1560,
        300
      ]
    },
    {
      "parameters": {
        "operation": "sendMessage",
        "chatId": "your_chat_id",
        "text": "={{ 'ðŸ“ˆ New Lead: ' + $json.company_name + '\\nContact: ' + $json.contact_name + '\\nIndustry: ' + $json.industry + '\\nLead ID: ' + $json.lead_id }}"
      },
      "name": "Send Telegram Alert",
      "type": "n8n-nodes-base.telegram",
      "typeVersion": 1,
      "position": [
        1780,
        300
      ]
    },
    {
      "parameters": {
        "operation": "update",
        "base": "your_base_id",
        "table": "Client Leads",
        "recordId": "={{ $json.lead_id }}",
        "fields": {
          "Status": "Outreach Sent",
          "Outreach Sent At": "={{ $now }}",
          "Next Action": "Follow up in 24 hours"
        }
      },
      "name": "Update Lead Status",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 1,
      "position": [
        2000,
        300
      ]
    }
