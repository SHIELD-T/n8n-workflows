  "nodes": [
    {
      "id": "webhook-trigger-1",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "position": [
        100,
        100
      ],
      "parameters": {
        "path": "week1-example",
        "httpMethod": "POST",
        "options": {
          "response": {
            "responseMode": "responseNode"
          }
        }
      }
    },
    {
      "id": "data-processing-1",
      "name": "Process Incoming Data",
      "type": "n8n-nodes-base.set",
      "position": [
        300,
        100
      ],
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "received_data",
              "value": "={{ $json }}"
            },
            {
              "name": "processed_at",
              "value": "={{ $now }}"
            },
            {
              "name": "data_type",
              "value": "webhook_data"
            }
          ]
        }
      }
    },
    {
      "id": "validation-1",
      "name": "Validate Data",
      "type": "n8n-nodes-base.if",
      "position": [
        500,
        100
      ],
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $json.received_data }}",
              "operation": "isNotEmpty"
            }
          ]
        }
      }
    },
    {
      "id": "success-response-1",
      "name": "Success Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "position": [
        700,
        100
      ],
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { \"status\": \"success\", \"processed_at\": $json.processed_at, \"data_type\": $json.data_type } }}"
      }
    },
    {
      "id": "error-response-1",
      "name": "Error Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "position": [
        700,
        300
      ],
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { \"status\": \"error\", \"message\": \"Invalid data received\", \"timestamp\": $now } }}"
      }
    }
  ],
  "connections": {
    "Webhook Trigger": {
      "main": [
        [
          {
            "node": "Process Incoming Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Process Incoming Data": {
      "main": [
        [
          {
            "node": "Validate Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Validate Data": {
      "main": [
        [
          {
            "node": "Success Response",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Error Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
