  "nodes": [
    {
      "id": "manual-trigger-3",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "position": [
        100,
        100
      ],
      "parameters": {}
    },
    {
      "id": "data-input-3",
      "name": "Input Data",
      "type": "n8n-nodes-base.set",
      "position": [
        300,
        100
      ],
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "raw_data",
              "value": "={{ $json.data || 'Sample data for processing' }}"
            },
            {
              "name": "processing_type",
              "value": "advanced"
            },
            {
              "name": "start_time",
              "value": "={{ $now }}"
            }
          ]
        }
      }
    },
    {
      "id": "data-cleaning-3",
      "name": "Data Cleaning",
      "type": "n8n-nodes-base.set",
      "position": [
        500,
        100
      ],
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "cleaned_data",
              "value": "={{ $json.raw_data.trim().toLowerCase() }}"
            },
            {
              "name": "cleaning_completed",
              "value": "={{ $now }}"
            },
            {
              "name": "cleaning_duration",
              "value": "={{ $now.diff($json.start_time, 'milliseconds') }}"
            }
          ]
        }
      }
    },
    {
      "id": "data-transformation-3",
      "name": "Data Transformation",
      "type": "n8n-nodes-base.set",
      "position": [
        700,
        100
      ],
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "word_count",
              "value": "={{ $json.cleaned_data.split(' ').length }}"
            },
            {
              "name": "character_count",
              "value": "={{ $json.cleaned_data.length }}"
            },
            {
              "name": "transformation_completed",
              "value": "={{ $now }}"
            },
            {
              "name": "transformation_duration",
              "value": "={{ $now.diff($json.cleaning_completed, 'milliseconds') }}"
            }
          ]
        }
      }
    },
    {
      "id": "data-analysis-3",
      "name": "Data Analysis",
      "type": "n8n-nodes-base.set",
      "position": [
        900,
        100
      ],
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "analysis_results",
              "value": "={{ { \"word_count\": $json.word_count, \"character_count\": $json.character_count, \"average_word_length\": Math.round($json.character_count / $json.word_count * 100) / 100 } }}"
            },
            {
              "name": "analysis_completed",
              "value": "={{ $now }}"
            },
            {
              "name": "total_duration",
              "value": "={{ $now.diff($json.start_time, 'milliseconds') }}"
            }
          ]
        }
      }
    },
    {
      "id": "result-formatting-3",
      "name": "Format Results",
      "type": "n8n-nodes-base.set",
      "position": [
        1100,
        100
      ],
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "final_report",
              "value": "ðŸ“Š Data Processing Report\n\n*Original Data:* {{ $json.raw_data }}\n*Cleaned Data:* {{ $json.cleaned_data }}\n*Word Count:* {{ $json.analysis_results.word_count }}\n*Character Count:* {{ $json.analysis_results.character_count }}\n*Average Word Length:* {{ $json.analysis_results.average_word_length }}\n*Total Processing Time:* {{ $json.total_duration }}ms"
            },
            {
              "name": "report_generated",
              "value": "={{ $now }}"
            },
            {
              "name": "status",
              "value": "completed"
            }
          ]
        }
      }
    }
  ],
